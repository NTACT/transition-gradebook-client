{
  "env": {
    "development": {
      "presets": [
        "react", 
        ["env", {
          "browsers": ["last 2 versions", "safari 7", "ie11"]
        }]
      ],
      "plugins": [
        "babel-plugin-styled-components",
        "transform-object-rest-spread",
        "transform-decorators-legacy",
        "transform-class-properties"
      ]
    },
    "production": {
      "presets": [
        "react",
        ["env", {
          "browsers": ["last 2 versions", "safari 7", "ie11"]
        }]
      ],
      "plugins": [
        "babel-plugin-styled-components",
        "transform-object-rest-spread",
        "transform-decorators-legacy",
        "transform-class-properties"
      ]
    }
  }
}